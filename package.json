{
  "name": "parseum",
  "description": "A markdown to HTML parser and editor built using Peggy.js, React, TailwindCSS and DaisyUI.",
  "author": "Visakan Kirubakaran",
  "version": "1.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/vikiru/parseum"
  },
  "keywords": [
    "Parseum",
    "markdown",
    "html",
    "parser",
    "editor",
    "peggy.js",
    "react",
    "tailwindcss",
    "daisyui"
  ],
  "bugs": {
    "url": "https://github.com/vikiru/parseum/issues"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env PORT=8080 react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env NODE_ENV=testing nyc --reporter=json-summary --reporter=text mocha test/test.js",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext js,jsx --fix-dry-run",
    "prettier": "eslint --ext js,jsx,pegjs --fix && prettier --write .",
    "parser": "peggy -o src/parser/parser.js src/parser/grammar.pegjs --format es",
    "deploy": "npm run build && surge ./build/ parseum.surge.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "defaults",
      ">0.01%",
      "not dead",
      "not op_mini all",
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ],
    "development": [
      "defaults",
      ">0.01%",
      "not dead",
      "not op_mini all",
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.24.1",
    "@tailwindcss/typography": "^0.5.12",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cross-env": "^7.0.3",
    "daisyui": "^4.9.0",
    "dompurify": "^3.0.11",
    "highlight.js": "^11.9.0",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "serve": "^14.2.1",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@peggyjs/eslint-plugin": "^1.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/parser": "^7.13.1",
    "autoprefixer": "^10.4.17",
    "chai": "^4.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-comment-length": "^1.7.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.0.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-sort-exports": "^0.9.1",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "istanbul-badges-readme": "^1.8.5",
    "mocha": "^10.4.0",
    "nodemon": "^3.1.0",
    "nyc": "^15.1.0",
    "open-cli": "^8.0.0",
    "peggy": "^4.0.2",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-pegjs": "^2.0.2",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "sinon": "^17.0.1"
  },
  "engines": {
    "node": ">=16.9"
  }
}
